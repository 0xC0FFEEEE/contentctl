

### STORIES ###

{% for story in objects %}
[analytic_story://{{ story.name }}]
category = {{ story.tags.getCategory_conf() }}
last_updated = {{ story.date }}
version = {{ story.version }}
references = {{ story.getReferencesListForJson() | tojson }}
maintainers = [{"company": "{{ story.author_company }}", "email": "-", "name": "{{ story.author }}"}]
spec_version = 3
searches = {{ (story.getDetectionNames() + story.getInvestigationNames()) | tojson }}
description = {{ story.description }}
{% if story.narrative is defined %}
narrative = {{ story.narrative }}
{% endif %}

{% endfor %}
### END STORIES ###

